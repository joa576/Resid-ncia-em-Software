{
    "manifest_version": 3,
    "name": "jetsales",
    "version": "1.0",
    "permissions": [
      "scripting", 
      "activeTab",
       "sidePanel"
      
    ],
    "action": {
      "default_title": "Click to open panel"
    },
    
    "side_panel": {
      "default_path": "index.html"
    },
    "background": {
      "service_worker": "background.js"
      
    }
    
  }